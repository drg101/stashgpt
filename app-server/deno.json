    "test": "deno test --allow-env"
  },
  "imports": {
    "@nestjs/common": "npm:@nestjs/common@^10.4.4",
    "@nestjs/core": "npm:@nestjs/core@^10.4.4",
    "@nestjs/platform-express": "npm:@nestjs/platform-express@^10.4.4",
    "@nestjs/testing": "npm:@nestjs/testing@^10.4.4",
    "@std/assert": "jsr:@std/assert@1",
    "@std/expect": "jsr:@std/expect@^1.0.4",
    "@std/testing": "jsr:@std/testing@^1.0.3",
    "firebase-admin": "npm:firebase-admin@^12.6.0"
  },
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
